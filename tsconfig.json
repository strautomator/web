{
    "compilerOptions": {
        "baseUrl": "",
        "rootDir": "src",
        "outDir": "server",
        "target": "es2024",
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "esModuleInterop": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "declaration": true,
        "skipLibCheck": true,
        "paths": {
            "strautomator-core": ["../core/", "node_modules/strautomator-core/"]
        }
    },
    "vueCompilerOptions": {
        "target": 2.7
    }
}
